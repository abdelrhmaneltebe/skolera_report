	<queryString>
		<![CDATA[select gs.*, courses.order,round(student_grade_in_category + 0.00000000001) new_student_grade_in_category, courses.order,
IF(courses.report_display_name is null , course_name,report_display_name) as report_display_name,

 (select additional_fields from students where students.id = gs.student_id) as additional_fields,

(select feedbacks.content from feedbacks 
inner join assignments on assignments.id = on_id and assignments.deleted_at is null and assignments.name like "%Teacher Comments%" and assignments.grading_period_id = 16
where on_type = "Assignment" and feedbacks.deleted_at is null and to_id = gs.student_id) as feedback

 from grades_summaries gs
 inner join levels on levels.id = level_id
 inner join courses on courses.deleted_at is null and course_id = courses.id and calculated_in_total = 1 and hide = 0
inner join users on users.deleted_at is null and users.actable_id = gs.student_id and users.user_type = "student" and (users.id in ($P!{USER_ID}) OR  $P{USER_ID}   = "-1")
where (gs.student_id in ($P!{STUDENT_ID}) OR $P{STUDENT_ID}  = "-1")
and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")
and main_grading_period_id= 16
and main_category_weight >0]]>
	</queryString>
