	<queryString>
		<![CDATA[select gs.*,courses.order,round(student_grade_in_category) new_student_grade_in_category, courses.order,
IF(courses.report_display_name is null , course_name,report_display_name) as report_display_name,

 (select if(additional_fields = "",null,additional_fields) from students where students.id = gs.student_id) additional_fields,

(select feedbacks.content from feedbacks 
inner join assignments on assignments.id = on_id and assignments.deleted_at is null and assignments.name like "%Comments%" 
where on_type = "Assignment" and feedbacks.deleted_at is null and to_id = gs.student_id and
gs.main_grading_period_id = assignments.grading_period_id) as feedback
 from grades_summaries gs
 inner join levels on levels.id = level_id
 inner join courses on courses.deleted_at is null and course_id = courses.id and calculated_in_total = 1 and hide = 0
inner join users on users.deleted_at is null and users.actable_id = gs.student_id and users.user_type = "student" and (users.id in ($P!{USER_ID}) OR  $P{USER_ID}   = "-1")
where section_id = 1
and (gs.student_id in ($P!{STUDENT_ID}) OR $P{STUDENT_ID}  = "-1")
and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")
and (gs.sub_grading_period_id in ($P!{SUB_GRADING_PERIOD_ID}) OR $P{SUB_GRADING_PERIOD_ID}  = "-1")
and (gs.main_grading_period_id in ($P!{MAIN_GRADING_PERIOD_ID}) OR $P{MAIN_GRADING_PERIOD_ID}  = "-1") 
and main_category_weight >0
group by student_id, course_id, main_grading_period_id]]>
	</queryString>
