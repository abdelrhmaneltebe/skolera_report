	<parameter name="LEVEL_CODE" class="java.lang.String">
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select *,
(select additional_fields from students where students.id = s.student_id) additional_fields,

(select feedbacks.content from feedbacks 
inner join assignments on assignments.id = on_id and assignments.deleted_at is null and assignments.name like "%Teacher Comments%" 
where on_type = "Assignment" and feedbacks.deleted_at is null and to_id = s.student_id and grading_period_id = 21) as feedback

from (
select gs.*, report_display_name, courses.order,
(student_grade_in_main_grading_period*6)/ main_grading_period_weight as student_grade 
from grades_summaries gs

 inner join levels on levels.id = gs.level_id and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")
 inner join users on users.deleted_at is null and users.actable_id = gs.student_id and users.user_type = "student" and (users.id in ($P!{USER_ID}) OR  $P{USER_ID}   = "-1")
inner join courses on courses.id = course_id and courses.deleted_at is null and courses.calculated_in_total = 1 and hide = 0  
where section_id = 1 and gs.level_id in (4,5,6,7,8)
group by student_id, course_id, main_grading_period_id)s]]>
	</queryString>
