		<parameter name="ST_ID" class="java.lang.String"/>
		<parameter name="MGP" class="java.lang.String"/>
		<queryString>
			<![CDATA[select status, sum(`count(id)`) as `count(id)`,student_id, st,Status_name,att_count from (select *,
Case 
 when s.st = 0 Then "Present"
 when s.st = 1 Then "Absent"
 when s.st = 2 Then "Late"
 when s.st = 3 Then "Excused"
END as Status_name ,
(select count(id) from attendances where student_id = $P{ST_ID} and status = 0 and attendances.deleted_at is null  and date> "2024-01-07" and date < "2024-03-27") att_count
from (select status, count(id), student_id
from attendances att
where deleted_at is null and student_id =  $P{ST_ID}  and status != 0 and date> "2023-10-08" and date < "2023-12-22"
group by status)att

right join (select attendances.status st from attendances where status != 0 group by status)s on s.st=att.status

union all 
select 
3 as status, 0 as `count(id)` , null as student_id, 3 as st, "Excused" as Status_name, (select count(id) from attendances where student_id = $P{ST_ID} and status = 0 and attendances.deleted_at is null and date> "2024-01-07" and date < "2024-03-27" ) att_count
)s
group by st]]>
		</queryString>
