	<queryString>
		<![CDATA[select student_name, level_name, student_id , course_group_name ,
(select comment from student_progress_reports spr
inner join progress_reports pr on pr.id = progress_report_id and pr.deleted_at is null and pr.title like "%Autumn%"
where spr.student_id = gs.student_id and spr.deleted_at is null) as Comment,
(SELECT count(id) absent_dayes FROM attendances where deleted_at is null and status in(1,3)  and attendances.student_id = gs.student_id) as absent_dayes,
(SELECT count(id) late_dayes FROM attendances where deleted_at is null and status = 2  and attendances.student_id = gs.student_id) as late_dayes
from grades_summaries gs
 inner join levels on levels.id = level_id
 inner join courses on courses.deleted_at is null and course_id = courses.id and calculated_in_total = 1 and hide = 0
inner join users on users.deleted_at is null and users.actable_id = gs.student_id and users.user_type = "student" and (users.id in ($P!{USER_ID}) OR  $P{USER_ID}   = "-1")
where (gs.student_id in ($P!{STUDENT_ID}) OR $P{STUDENT_ID}  = "-1")
and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")

group by student_id]]>
	</queryString>
