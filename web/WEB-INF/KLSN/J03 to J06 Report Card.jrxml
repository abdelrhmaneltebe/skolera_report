	<queryString>
		<![CDATA[select gs.*, courses.total_grade, courses.calculated_in_total, courses.hide, courses.order, levels.level_code,
IF(report_display_name is null or report_display_name = "" ,course_name,report_display_name) as report_display_name
 from grades_summaries gs
 inner join courses on courses.id = course_id and courses.deleted_at is null and courses.hide = 0 and courses.calculated_in_total = 1
inner join levels on levels.id = gs.level_id and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")
inner join users on users.deleted_at is null and users.actable_id = gs.student_id and users.user_type = "student" and (users.id in ($P!{USER_ID}) OR  $P{USER_ID}   = "-1") 
where (gs.main_grading_period_id in ($P!{MAIN_GRADING_PERIOD_ID}) OR $P{MAIN_GRADING_PERIOD_ID}  = "-1")
 group by student_id, course_id, main_grading_period_id]]>
	</queryString>
