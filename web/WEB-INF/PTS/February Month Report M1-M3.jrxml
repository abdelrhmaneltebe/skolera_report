(gs.student_grade_in_category / gs.main_category_weight) * 10 as grade,
ifnull(spr.comment, "N/A") as general_comment,
(select count(id) from attendances where students.id = attendances.student_id and attendances.deleted_at is null and attendances.status in (1,3) and attendances.date between gp.start_date and gp.end_date) as absence_days,

ifnull((select sprdc1.comment from student_progress_report_domain_comments sprdc1
