	<queryString>
		<![CDATA[select *,  (select name from levels where id = s.level_id+1) as next_level from (select 
student_id, users.name as student_name, student_date_of_birth,username,if(home_address is null or home_address ="0"
	or student_id in (select students.id from (select *,
 LENGTH(home_address) - LENGTH(REPLACE(home_address, ',', '')) AS comma_count
 from users where user_type = "student" and deleted_at is null)s
 inner join students on students.id = actable_id and students.deleted_at is null
 where comma_count != 3)
	," , , , ",home_address
) as home_address,dateofbirth,
course_id, course_name, courses.order, courses.report_display_name, course_name as parent_course, course_group_name,
level_name, level_code,gs.level_id,
main_grading_period_id, main_grading_period_name, main_grading_period_weight,
student_grade_in_main_grading_period
from grades_summaries gs
inner join levels on levels.id = level_id and levels.deleted_at is null
inner join courses on courses.deleted_at is null and course_id = courses.id and courses.hide = 0 and code not like "ARB-SVPR%"
inner join users on users.deleted_at is null and users.actable_id = gs.student_id and users.user_type = "student" and (users.id in ($P!{USER_ID}) OR  $P{USER_ID}   = "-1")
where (gs.student_id in ($P!{STUDENT_ID}) OR $P{STUDENT_ID}  = "-1")
and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")

group by  student_id , course_id, main_grading_period_id 

union all 

select 
student_id, users.name as student_name, student_date_of_birth,username,
if(home_address is null or home_address ="0"
	or student_id in (select students.id from (select *,
 LENGTH(home_address) - LENGTH(REPLACE(home_address, ',', '')) AS comma_count
 from users where user_type = "student" and deleted_at is null)s
 inner join students on students.id = actable_id and students.deleted_at is null
 where comma_count != 3)
	," , , , ",home_address
) as home_address
,dateofbirth,
course_id, TRIM(main_category_name) as course_name, courses.order, courses.report_display_name, course_name as parent_course,course_group_name,
level_name, level_code,gs.level_id,
main_grading_period_id, main_grading_period_name, main_grading_period_weight,
student_grade_in_category as student_grade_in_main_grading_period
from grades_summaries gs
inner join levels on levels.id = level_id and levels.deleted_at is null
inner join courses on courses.deleted_at is null and course_id = courses.id and courses.hide = 0 and code like "ARB-SVPR%"
inner join users on users.deleted_at is null and users.actable_id = gs.student_id and users.user_type = "student" and (users.id in ($P!{USER_ID}) OR  $P{USER_ID}   = "-1")
where (gs.student_id in ($P!{STUDENT_ID}) OR $P{STUDENT_ID}  = "-1")
and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")

and main_category_weight > 0 
group by  student_id , course_id, main_grading_period_id , main_category_id)s]]>
	</queryString>
