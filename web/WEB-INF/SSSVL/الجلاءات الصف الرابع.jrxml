	<subDataset name="att" uuid="1ccf655d-acf6-4a11-927b-461561550dc7">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="FBIC"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="ST_ID" class="java.lang.String"/>
		<queryString>
			<![CDATA[select sum(att_count) as att_count,student_id,attendance_type, attendance_p,main_grading_period_name from  (select count(id) att_count, student_id,attendance_type, attendance_p,main_grading_period_name from (select 
attendances.id,
attendances.student_id,
if(status =0 , 0,
if(status =1,1,
if(status =2,99,2))) as attendance_type,
if(status =0 , 1,0) as attendance_p,
 main_grading_period_name from attendances
inner join (select student_id,main_grading_period_id, main_grading_period_name, start_date, end_date from grades_summaries 
inner join grading_periods gp on gp.id = main_grading_period_id
group by student_id, main_grading_period_id)s on  s.student_id = attendances.student_id 
where deleted_at is null and status in (0,1,3) and timetable_slot_id is null and attendances.student_id = $P{ST_ID}  )s
group by student_id,attendance_type, attendance_p,main_grading_period_name

union all 

select 
0 as att_count,
student_id,
2 as attendance_type,
0 as attendance_p,
main_grading_period_name from grades_summaries 
inner join grading_periods gp on gp.id = main_grading_period_id
where student_id = $P{ST_ID}
group by student_id, main_grading_period_id 

union all 

select 
0 as att_count,
student_id,
1 as attendance_type,
0 as attendance_p,
main_grading_period_name from grades_summaries 
inner join grading_periods gp on gp.id = main_grading_period_id
where student_id = $P{ST_ID}
group by student_id, main_grading_period_id 

union all 

select 
0 as att_count,
student_id,
0 as attendance_type,
1 as attendance_p,
main_grading_period_name from grades_summaries 
inner join grading_periods gp on gp.id = main_grading_period_id
where student_id = $P{ST_ID}
group by student_id, main_grading_period_id )s group by student_id,attendance_type, attendance_p,main_grading_period_name]]>
		</queryString>
