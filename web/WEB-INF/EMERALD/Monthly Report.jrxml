	<queryString>
		<![CDATA[select 
levels.id as level_id, levels.name as level_name, 
gs.course_group_name,
students.id as student_id, users.name as student_name,
courses.id as course_id, 
IF(courses.report_display_name is null, courses.name, courses.report_display_name) as course_name,
courses.calculated_in_total, courses.hide, courses.order as course_order,
gs.main_grading_period_id, gs.main_grading_period_name, gs.sub_grading_period_id, gs.sub_grading_period_name,
(student_grade_in_sub_grading_period/sub_grading_period_weight) * 100 as student_grade_from100all,
(student_grade_in_sub_grading_period/sub_grading_period_weight) * courses.total_grade as student_grade,
sprdc.comment as teacher_comment,
spr.comment as general_comment,
courses.total_grade as sub_weight,
courses.code as course_code
from grades_summaries gs
inner join students on students.id = gs.student_id and students.deleted_at is null
inner join courses on courses.id = gs.course_id and courses.deleted_at is null and courses.hide = 0
inner join levels on levels.id = gs.level_id and levels.deleted_at is null and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")
inner join users on users.deleted_at is null and users.actable_id = gs.student_id and user_type = "student" and (users.id in ($P!{USER_ID}) OR  $P{USER_ID}   = "-1")
left join domains on courses.id = domains.course_id and domains.deleted_at is null
left join progress_reports pr on pr.id = domains.progress_report_id and pr.deleted_at is null
left join student_progress_reports spr on spr.student_id = students.id and spr.deleted_at is null
left join student_progress_report_domain_comments sprdc on sprdc.student_progress_report_id = spr.id and sprdc.domain_id = domains.id and sprdc.deleted_at is null
where gs.sub_grading_period_id in (3,4,7,8)
and level_name not like "%KG%"
and (gs.sub_grading_period_id in ($P!{SUB_GRADING_PERIOD_ID}) OR $P{SUB_GRADING_PERIOD_ID}  = "-1")
and (gs.main_grading_period_id in ($P!{MAIN_GRADING_PERIOD_ID}) OR $P{MAIN_GRADING_PERIOD_ID}  = "-1") 
group by students.id, courses.id, sub_grading_period_id]]>
