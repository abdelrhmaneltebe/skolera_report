	<subDataset name="Overall Trimester" uuid="e300555f-caf2-49f8-b380-e36033031404">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="SGIS"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="Student_ID" class="java.lang.String"/>
		<parameter name="Dynamic_GP" class="java.lang.String"/>
		<queryString>
			<![CDATA[select s.*, dc.title, dc.order
from (SELECT 
            gs.student_id, 
            gs.student_name, 
            gs.level_id, 
            gs.level_name, 
            gs.course_group_name,
            gs.course_name,
            gs.main_grading_period_id, 
            ROUND((gs.student_grade_in_main_grading_period / gs.main_grading_period_weight) * 100, 2) as trimester_grade,
            CASE 
    WHEN ROUND((gs.student_grade_in_main_grading_period / gs.main_grading_period_weight) * 100, 2) >= 90 THEN "A"
    WHEN ROUND((gs.student_grade_in_main_grading_period / gs.main_grading_period_weight) * 100, 2) >= 80 THEN "B"
    WHEN ROUND((gs.student_grade_in_main_grading_period / gs.main_grading_period_weight) * 100, 2) >= 70 THEN "C"
    WHEN ROUND((gs.student_grade_in_main_grading_period / gs.main_grading_period_weight) * 100, 2) >= 60 THEN "D"
    ELSE "F"
END AS letter_grade
        FROM 
            grades_summaries gs
        WHERE 
            gs.course_name LIKE "%STEM%" 
            AND gs.level_id != 39
            AND gs.student_id =  $P{Student_ID} 
        GROUP BY 
            gs.student_id,
            gs.level_id, 
            gs.main_grading_period_id
		order by 
			gs.level_id, 
            gs.course_group_name,
            gs.student_name,
            gs.main_grading_period_id)s
            inner join domains on domains.title = s.letter_grade and domains.deleted_at is null
            inner join domain_competencies dc on dc.domain_id = domains.id and dc.deleted_at is null
            order by
            s.level_id, 
