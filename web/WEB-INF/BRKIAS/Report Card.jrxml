		<parameter name="Student_id" class="java.lang.String"/>
		<parameter name="MGP" class="java.lang.String"/>
		<queryString>
			<![CDATA[select student_id, count(id), status from attendances 
where student_id = $P{Student_id}   and status = 1 
and date>=(select start_date from grading_periods where id = $P{MGP}  ) 
and date<=(select end_date from grading_periods where id = $P{MGP}  )]]>
		</queryString>
		<field name="student_id" class="java.lang.Integer">
			<property name="com.jaspersoft.studio.field.label" value="student_id"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="attendances"/>
		</field>
