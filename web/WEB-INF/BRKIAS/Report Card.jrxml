		</field>
	</subDataset>
	<parameter name="MAIN_GRADING_PERIOD_ID" class="java.lang.String">
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<parameter name="STUDENT_ID" class="java.lang.String">
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<parameter name="LEVEL_CODE" class="java.lang.String">
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<parameter name="ABSOLUTE_PATH" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="USER_ID" class="java.lang.String">
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select student_id, (select name from users where deleted_at is null and actable_id = student_id and users.user_type = "student") as student_name, 
gs.level_id,
course_id, course_name,
main_grading_period_id, main_grading_period_name,
main_category_id, main_category_name, main_category_weight, 
ROUND(student_grade_in_category,0) as student_grade_in_category,
level_name, course_group_id, course_group_name,
concat(calculated_in_total, hide) course_cal, courses.order,

(select users.name from course_groups_teachers cgt 
inner join course_groups cg on cg.id = course_group_id and cg.deleted_at is null
inner join courses as co on co.id = cg.course_id and co.deleted_at is null and co.name like "home%"
inner join users on users.actable_id = teacher_id and users.user_type = "teacher" and users.deleted_at is null
where cgt.deleted_at is null
and co.level_id = gs.level_id and cg.name COLLATE utf8_unicode_ci = gs.course_group_name COLLATE utf8_unicode_ci ) as teacher_name,

(select ROUND(sum(grade),0) from grade_items gi
inner join grade_submissions gss on gss.grade_item_id = gi.id and gss.deleted_at is null
where category_id = main_category_id and gi.deleted_at is null and gss.student_id = gs.student_id 
and gi.course_group_id = gs.course_group_id and grading_period_id = main_grading_period_id) as item_grade,

(select ROUND(sum(max_grade),0) from grade_items gi
inner join grade_submissions gss on gss.grade_item_id = gi.id and gss.deleted_at is null
where category_id = main_category_id and gi.deleted_at is null and gss.student_id = gs.student_id 
and gi.course_group_id = gs.course_group_id and grading_period_id = main_grading_period_id) as max_grade,

(select feedbacks.content from feedbacks 
inner join assignments on assignments.id = on_id and assignments.deleted_at is null and name like "Teacher comment%"
where on_type = "Assignment" and feedbacks.deleted_at is null and to_id = student_id
and grading_period_id = main_grading_period_id) as feedback

 from grades_summaries gs
inner join courses on courses.id = course_id and courses.deleted_at is null and courses.order !=99
inner join levels on levels.id = gs.level_id
inner join users on users.deleted_at is null and users.actable_id = gs.student_id and users.user_type = "student" and (users.id in ($P!{USER_ID}) OR  $P{USER_ID}   = "-1")
where (gs.student_id in ($P!{STUDENT_ID}) OR $P{STUDENT_ID}  = "-1")
and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")
and (gs.main_grading_period_id in ($P!{MAIN_GRADING_PERIOD_ID}) OR $P{MAIN_GRADING_PERIOD_ID}  = "-1")
and main_category_weight >0]]>
	</queryString>
