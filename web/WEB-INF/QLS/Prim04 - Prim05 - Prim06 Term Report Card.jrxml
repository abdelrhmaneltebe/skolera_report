	<queryString>
		<![CDATA[select *,
cg.name as class,
concat(ut.firstname," ", ut.lastname) as en_name
from
(
select
student_id, student_name, level_id, level_name, course_group_name, course_id, course_name, total_grade, pass_limit,course_order, main_category_id, main_category_name,
-- oct-nov-dec sub periods weight + mid term weight 70-- 
70 as main_category_weight,
(sum(student_grade)/sum(main_category_weight)) * 70 as student_grade,
(sum(student_grade) / sum(year_work.weight)) * (pass_limit) as year_work_grade,
1 as item_type
from (
select 
student_id, student_name, level_id, level_name, course_group_name, course_id, course_name, total_grade, pass_limit,course_order, main_category_id, main_category_name,
-- oct-nov-dec sub periods weight 40-- 
40 as main_category_weight,
ROUND(ond.weight * 3) as weight,
(sum(student_grade)/sum(main_category_weight)) * ROUND(ond.weight * 3) as student_grade
from
(-- sub period categories start--
select
users.actable_id as student_id, users.name as student_name,
levels.id as level_id, levels.name as level_name,
RIGHT(students.batch_name, 1) as course_group_name,
courses.id as course_id,
IF(courses.report_display_name is null, courses.name, courses.report_display_name) as course_name,
courses.total_grade, courses.pass_limit,
courses.order as course_order,
gs.main_category_id, gs.main_category_name, gs.main_category_weight,
gs.sub_grading_period_weight as weight,
AVG(gs.student_grade_in_category) as student_grade
from grades_summaries gs
inner join students on students.id = gs.student_id and students.deleted_at is null
inner join levels on levels.id = gs.level_id and levels.deleted_at is null and levels.id in (4,5,6)
inner join courses on courses.id = gs.course_id and courses.deleted_at is null
inner join users on users.actable_id = gs.student_id and users.deleted_at is null and users.actable_type = "student"
inner join graded_category gc on gc.category_id = gs.main_category_id and gc.course_group_id = gs.course_group_id and gc.grading_period_id = gs.sub_grading_period_id
where gs.main_category_id not in (39,43,44) and gs.sub_grading_period_id in (58,59,60) and gs.main_grading_period_id = 57
group by gs.student_id, gs.course_id, gs.main_category_id
-- sub period categoried end--

UNION ALL


-- weekly exam start--
select
users.actable_id as student_id, users.name as student_name,
levels.id as level_id, levels.name as level_name,
RIGHT(students.batch_name, 1) as course_group_name,
courses.id as course_id,
IF(courses.report_display_name is null, courses.name, courses.report_display_name) as course_name,
courses.total_grade, courses.pass_limit,
courses.order as course_order,
gi.category_id as main_category_id, "Weekly Exam" as main_category_name, cgpc.weight as main_category_weight,
cgp.weight as weight,
AVG((gss_main.grade/gi.max_grade) * cgpc.weight) as student_grade
from grade_submissions gss_main
inner join grade_items gi on gi.id = gss_main.grade_item_id and gi.category_id in (39,44) and gi.deleted_at is null
inner join categories on categories.id = gi.category_id and categories.deleted_at is null
inner join students on students.id = gss_main.student_id and students.deleted_at is null
inner join levels on levels.id = students.level_id and levels.deleted_at is null
inner join users on users.actable_id = students.id and users.deleted_at is null and users.actable_type = "student"
inner join courses on courses.id = gss_main.course_id and courses.deleted_at is null and courses.level_id in (4,5,6)
inner join grading_periods gp on gp.id = gi.grading_period_id and gp.deleted_at is null
inner join courses_grading_periods cgp on cgp.course_id = courses.id and cgp.grading_period_id = gp.id and cgp.deleted_at is null
inner join course_grading_periods_categories cgpc on cgpc.category_id = categories.id and cgpc.courses_grading_period_id = cgp.id and cgpc.deleted_at is null
where gss_main.deleted_at is null
and gi.grading_period_id in (58,59,60)
and gss_main.id not in (

select * from (SELECT gss_outer.id
FROM grade_submissions gss_outer
INNER JOIN students ON students.id = gss_outer.student_id 
    AND students.deleted_at IS NULL 
    AND students.level_id IN (4,5,6)
INNER JOIN grade_items gi ON gi.id = gss_outer.grade_item_id
    AND gi.category_id in (39,44)
    AND gi.deleted_at IS NULL
INNER JOIN (
    SELECT 
        gss_inner.student_id, 
        gss_inner.course_id, 
        MIN((gss_inner.grade / gi_inner.max_grade) * 100) AS min_percentage
    FROM grade_submissions gss_inner
    INNER JOIN grade_items gi_inner ON gi_inner.id = gss_inner.grade_item_id 
        AND gi_inner.category_id in (39,44)
        AND gi_inner.deleted_at IS NULL 
    WHERE gss_inner.deleted_at IS NULL
    GROUP BY gss_inner.student_id, gss_inner.course_id
    HAVING COUNT(*) > 1
) min_percentages ON gss_outer.student_id = min_percentages.student_id 
    AND gss_outer.course_id = min_percentages.course_id 
    AND round(((gss_outer.grade / gi.max_grade) * 100),1) = round(min_percentages.min_percentage,1)
WHERE gss_outer.deleted_at IS NULL
GROUP BY gss_outer.student_id, gss_outer.course_id
ORDER BY gss_outer.student_id, gss_outer.course_id, gss_outer.grade
)s

)
group by students.id, courses.id
-- weekly exam end--
)ond
group by student_id, course_id


UNION ALL

-- Mid Term Query start--
select
student_id, student_name, level_id, level_name, course_group_name, course_id, course_name, total_grade, pass_limit,course_order, main_category_id, main_category_name,
-- midterm sub period weight 30-- 
30 as main_category_weight,
midterm.weight as weight,
(sum(student_grade)/sum(main_category_weight)) * midterm.weight as student_grade
from
(select 
users.actable_id as student_id, users.name as student_name,
levels.id as level_id, levels.name as level_name,
RIGHT(students.batch_name, 1) as course_group_name,
courses.id as course_id,
IF(courses.report_display_name is null, courses.name, courses.report_display_name) as course_name,
courses.total_grade, courses.pass_limit,
courses.order as course_order,
gs.main_category_id, gs.main_category_name, gs.main_category_weight,
gs.sub_grading_period_weight as weight,
gs.student_grade_in_category as student_grade
from grades_summaries gs
inner join students on students.id = gs.student_id and students.deleted_at is null
inner join levels on levels.id = gs.level_id and levels.deleted_at is null and levels.id in (4,5,6)
inner join courses on courses.id = gs.course_id and courses.deleted_at is null
inner join users on users.actable_id = gs.student_id and users.deleted_at is null and users.actable_type = "student"
where gs.sub_grading_period_id = 61 and gs.main_category_id not in (43,39,44))midterm
group by student_id, course_id
-- Mid Term Query end--
)year_work
group by student_id, course_id

UNION ALL

-- Term Exam Query -- 
select
users.actable_id as student_id, users.name as student_name,
levels.id as level_id, levels.name as level_name,
RIGHT(students.batch_name, 1) as course_group_name,
courses.id as course_id,
IF(courses.report_display_name is null, courses.name, courses.report_display_name) as course_name,
courses.total_grade, (courses.total_grade - courses.pass_limit) as pass_limit,
courses.order as course_order,
gs.main_category_id, gs.main_category_name, gs.sub_grading_period_weight as main_category_weight,
(sum(gs.student_grade_in_category)/sum(gs.main_category_weight)) * gs.sub_grading_period_weight as student_grade,
(gs.student_grade_in_category / gs.main_category_weight) * (courses.total_grade - courses.pass_limit) as year_work_grade,
2 as item_type


from grades_summaries gs
inner join students on students.id = gs.student_id and students.deleted_at is null
inner join levels on levels.id = gs.level_id and levels.deleted_at is null and levels.id in (4,5,6)
inner join courses on courses.id = gs.course_id and courses.deleted_at is null
inner join users on users.actable_id = gs.student_id and users.deleted_at is null and users.actable_type = "student"
where main_category_id = 51 and main_grading_period_id = 57
group by students.id, courses.id
-- term exam Query -- 
)k
inner join students on students.id = k.student_id and students.deleted_at is null
inner join levels on levels.id = k.level_id and levels.deleted_at is null and levels.id in (4,5,6) and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")
inner join courses on courses.id = k.course_id and courses.deleted_at is null
inner join course_groups cg on cg.course_id =courses.id and cg.deleted_at is null
inner join student_course_groups scg on scg.student_id = students.id and scg.course_group_id = cg.id and scg.deleted_at is null
inner join users on users.actable_id = k.student_id and users.deleted_at is null and users.actable_type = "student" and (users.id in ($P!{USER_ID}) OR  $P{USER_ID}   = "-1")
inner join user_translations ut on ut.user_id = users.id and ut.locale = "en"

where courses.hide = 0

order by k.level_id, cg.name, student_name, course_order]]>
	</queryString>
