	<queryString>
		<![CDATA[select concat(firstname," ",lastname) as student_name, users.*, students.batch_name,students.level_id, g.student_total from users  
 inner join students on students.deleted_at is null and students.id = users.actable_id
 inner join user_translations ut on ut.user_id = users.id and ut.locale = "en"
 inner join (select student_id, sum(student_total) / count(course_id) student_total from (select *, 
 	sum(((student_grade_in_main_grading_period*100)/main_grading_period_weight)+IF((course_name like "%English%" or 
 course_name like "%Science%" or 
 course_name like "%Social Studies%" or 
 course_name like "%Math%" or 
  course_name like "%French%" or
  course_name like "%Biology%" or 
   course_name like "%Physics%" or 
    course_name like "%Chemistry%" ) and main_grading_period_name = "Trimester 3" and level_id !=13 and level_id !=14
 ,2,0))/count(main_grading_period_id) as student_total
from ( select grades_summaries.* from grades_summaries
inner join courses on courses.deleted_at is null and courses.id = course_id and courses.calculated_in_total= 1
where (grades_summaries.main_grading_period_id in ($P!{MAIN_GRADING_PERIOD_ID}) OR $P{MAIN_GRADING_PERIOD_ID}  = "-1")
 group by student_id, course_id, main_grading_period_id
 )s group by student_id, course_id)t
 group by student_id)g on g.student_id = students.id
 inner join levels on levels.deleted_at is null and levels.id = students.level_id  and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")
 where users.deleted_at is null and users.user_type= "student" 
 and (users.id in ($P!{USER_ID}) OR $P{USER_ID}  = "-1")]]>
	</queryString>
