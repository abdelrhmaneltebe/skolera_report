		<queryString>
			<![CDATA[select  
	school_name, student_id, main_grading_period_name, 
	sum(student_grade)/count(course_name)
	, main_grading_period_id
from (select (select school_description from schools limit 1) as school_name, student_id,  main_grading_period_id,
 student_name, course_name, main_grading_period_name,(student_grade_in_main_grading_period*100)/main_grading_period_weight student_grade, student_grade_in_main_grading_period, course_group_name
 from grades_summaries 
