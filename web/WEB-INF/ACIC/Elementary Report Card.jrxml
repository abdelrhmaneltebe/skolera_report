	<subDataset name="att" uuid="fa98117a-4a49-453b-8758-a09b60e750a7">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="ACIC"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="Parameter1" class="java.lang.String"/>
		<parameter name="MGP" class="java.lang.String"/>
		<queryString>
			<![CDATA[select 1 as id, gp_id, name,status from attendances 
right join (select start_date, end_date, name,id as gp_id  from grading_periods where deleted_at is null group by start_date)s on attendances.date >= start_date and attendances.date <= end_date
where attendances.deleted_at is null
and (status in (1,2) or status is null) and (student_id =  $P{Parameter1} )

union all 
select 0 as id , 27 as gp_id , "Trimester 1" as name , 1 as status
union all 
select 0 as id , 28 as gp_id , "Trimester 2" as name , 1 as status
union all 
select 0 as id , 29 as gp_id , "Trimester 3" as name , 1 as status

union all 
select 0 as id , 27 as gp_id , "Trimester 1" as name , 2 as status
union all 
select 0 as id , 28 as gp_id , "Trimester 2" as name , 2 as status
union all 
select 0 as id , 29 as gp_id , "Trimester 3" as name , 2 as status]]>
		</queryString>
