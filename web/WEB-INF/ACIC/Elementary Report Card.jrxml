		</field>
	</subDataset>
	<subDataset name="comm" uuid="195e9105-5bc0-4c75-bd9c-33539ca8443f">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="ACIC"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="Parameter1" class="java.lang.String"/>
		<parameter name="MGP" class="java.lang.String"/>
		<queryString>
			<![CDATA[select s.*, if(pr.title is null , "",comment) as comment, pr.title from (select 
student_id, main_grading_period_id, main_grading_period_name
from grades_summaries
where student_id = $P{Parameter1}   group by student_id, main_grading_period_id)s
left join student_progress_reports spr on spr.student_id = s.student_id and spr.deleted_at is null
left join progress_reports pr on pr.id = progress_report_id and pr.title like concat("%",main_grading_period_name,"%")]]>
		</queryString>
		<field name="student_id" class="java.lang.Integer">
			<property name="com.jaspersoft.studio.field.label" value="student_id"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="grades_summaries"/>
