	<parameter name="ABSOLUTE_PATH" class="java.lang.String">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="MAIN_GRADING_PERIOD_ID" class="java.lang.String">
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<parameter name="LEVEL_CODE" class="java.lang.String">
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<parameter name="USER_ID" class="java.lang.String">
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select gs.*,
group_concat( DISTINCT main_grading_period_name) GP_names,
 
 (select group_concat(users.name) from student_course_groups scg 
inner join course_groups cg on cg.deleted_at is null and cg.id = scg.course_group_id
inner join courses on courses.id = cg.course_id and courses.deleted_at is null and courses.name like "English%" 
inner join course_groups_teachers cgt on cgt.course_group_id = cg.id and cgt.deleted_at is null
inner join users on users.actable_id = teacher_id and user_type = "teacher" and users.deleted_at is null
where scg.deleted_at is null and scg.student_id = gs.student_id
group by student_id) as teacher_name

from grades_summaries gs
inner join courses on courses.id = gs.course_id and courses.deleted_at is null and courses.order <5 and courses.order >0 and calculated_in_total = 1 and hide = 0
 inner join levels on levels.deleted_at is null and levels.id = gs.level_id  and (levels.level_code in ($P!{LEVEL_CODE}) OR $P{LEVEL_CODE}  = "-1")
 inner join users on users.actable_id = gs.student_id and users.deleted_at is null and users.user_type= "student" 
 and (users.id in ($P!{USER_ID}) OR $P{USER_ID}  = "-1")
where  sub_category_id is not null 
and (gs.main_grading_period_id in ($P!{MAIN_GRADING_PERIOD_ID}) OR $P{MAIN_GRADING_PERIOD_ID}  = "-1")
group by student_id]]>
	</queryString>
